{
    "name": "led-controller",
    "version": "1.0.0",
    "description": "Arduino / PlatformIO library for the Current Electric Vehicles LED Controller (C111)",
    "license": "MIT",
    "homepage": "https://github.com/Current-Electric-Vehicles/led-controller",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "scripts": {
        "preinstall": "install-board.py"
    },
    "build": {
        "flags": [
            "-std=gnu++17"
        ],
        "unflags": [
            "-std=gnu++11"
        ]
    },
    "dependencies": {
        "Arduino-TMP1075": "https://github.com/PatrickBaus/Arduino-TMP1075",
        "MCP23008": "https://github.com/RobTillaart/MCP23008",
        "arduino-CAN": "https://github.com/sandeepmistry/arduino-CAN"
    }
  }
  