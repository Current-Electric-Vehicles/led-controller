{
  "build": {
    "arduino":{
      "ldscript": "esp32_out.ld"
    },
    "core": "esp32",
    "extra_flags": [
      "-DARDUINO_ARCH_ESP32",
      "-DARDUINO_CURRENT_EV_C111",
      "-DARDUINO_ESP32_DEV"
    ], 
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "mcu": "esp32",
    "variant": "esp32"
  },
  "connectivity": [
    "wifi",
    "bluetooth",
    "can"
  ],
  "debug": {
    "openocd_board": "esp-wroom-32.cfg"
  },
  "frameworks": [
    "arduino",
    "arduino3",
    "espidf"
  ],
  "name": "Current Electric Vehicles C111 ESP32",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "wait_for_upload_port": true,
    "speed": 460800,
    "native_usb": true
  },
  "url": "https://current-la.com",
  "vendor": "Current Electric Vehicles"
}
